   "source": [
    "import cv2\n",
    "#path\n",
    "path = 'images/lagos.jpg'\n",
    "\n",
    "#reading image using imread() function\n",
    "img = cv2.imread(path)\n",
    "\n",
    "window_name = \"Display Image\"\n",
    "\n",
    "# Display image\n",
    "cv2.imshow(window_name, img)\n",
    "\n",
    "# Hold the window\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
